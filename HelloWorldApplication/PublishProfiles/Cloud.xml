<?xml version="1.0" encoding="utf-8"?>
<PublishProfile xmlns="http://schemas.microsoft.com/2015/05/fabrictools">
  <!-- ClusterConnectionParameters allows you to specify the PowerShell parameters to use when connecting to the Service Fabric cluster.
       Valid parameters are any that are accepted by the Connect-ServiceFabricCluster cmdlet.
       
       For a local cluster, you would typically not use any parameters.
         For example: <ClusterConnectionParameters />
  -->
  <ClusterConnectionParameters ConnectionEndpoint="" X509Credential="true" ServerCertThumbprint="" FindType="FindByThumbprint" FindValue="" StoreLocation="CurrentUser" StoreName="My" />
  <ApplicationParameterFile Path="..\ApplicationParameters\Cloud.xml" />
  <CopyPackageParameters CompressPackage="true" />
</PublishProfile>